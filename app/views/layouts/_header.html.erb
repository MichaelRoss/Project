<header class="navbar navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container">
      <%= link_to "Project", root_path, id: "logo" %>
      <nav>
        <ul class="nav pull-right">
          <li><%= link_to "Home", root_path,  :id=> "navoption" %></li>
          <li><%= link_to "Help", help_path,:id=> "navoption"  %></li>
          <% if signed_in? %>
            <li>
              <%= link_to "Sign out", signout_path, method: "delete", :id=> "navoption"  %>
            </li>
          <% else %>
            <li><%= link_to "Sign in", signin_path, :id=> "navoption"  %></li>
          <% end %>
        </ul>
      </nav>
    </div>
  </div>
</header>